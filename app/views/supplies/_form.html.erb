<%= simple_form_for(@supply) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :number_space %>
    <%= f.input :hourly_rate %>
    <%= f.input :daily_rate %>
    <%= f.input :weekly_rate %>
    <%= f.input :monthly_rate %>
    <%= f.association :region %>
    <%= f.association :state %>
    <%= f.association :zone %>
    <%= f.input :address %>
    <%= f.input :active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
