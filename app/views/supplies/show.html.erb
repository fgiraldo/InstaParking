<%- model_class = Supply -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:number_space) %>:</strong></dt>
  <dd><%= @supply.number_space %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hourly_rate) %>:</strong></dt>
  <dd><%= @supply.hourly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:daily_rate) %>:</strong></dt>
  <dd><%= @supply.daily_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:weekly_rate) %>:</strong></dt>
  <dd><%= @supply.weekly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:monthly_rate) %>:</strong></dt>
  <dd><%= @supply.monthly_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:region_id) %>:</strong></dt>
  <dd><%= @supply.region_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state_id) %>:</strong></dt>
  <dd><%= @supply.state_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:zone_id) %>:</strong></dt>
  <dd><%= @supply.zone_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @supply.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:active) %>:</strong></dt>
  <dd><%= @supply.active %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              supplies_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_supply_path(@supply), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              supply_path(@supply),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
