<p id="notice"><%= notice %></p>

<h1>Listing Supplies</h1>

<table>
  <thead>
    <tr>
      <th>Number space</th>
      <th>Hourly rate</th>
      <th>Daily rate</th>
      <th>Weekly rate</th>
      <th>Monthly rate</th>
      <th>Region</th>
      <th>State</th>
      <th>Zone</th>
      <th>Address</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supplies.each do |supply| %>
      <tr>
        <td><%= supply.number_space %></td>
        <td><%= supply.hourly_rate %></td>
        <td><%= supply.daily_rate %></td>
        <td><%= supply.weekly_rate %></td>
        <td><%= supply.monthly_rate %></td>
        <td><%= supply.region %></td>
        <td><%= supply.state %></td>
        <td><%= supply.zone %></td>
        <td><%= supply.address %></td>
        <td><%= supply.active %></td>
        <td><%= link_to 'Show', supply %></td>
        <td><%= link_to 'Edit', edit_supply_path(supply) %></td>
        <td><%= link_to 'Destroy', supply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supply', new_supply_path %>
